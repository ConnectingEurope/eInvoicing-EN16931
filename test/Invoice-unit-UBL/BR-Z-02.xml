				<cac:Party>
					<cac:PartyTaxScheme>
						<cbc:CompanyID>NO123456789</cbc:CompanyID>
						<cac:TaxScheme>
							<cbc:ID>VAT</cbc:ID>
						</cac:TaxScheme>
					</cac:PartyTaxScheme>
				</cac:Party>
			</cac:AccountingSupplierParty>

